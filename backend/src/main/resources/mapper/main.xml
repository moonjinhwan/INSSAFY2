<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.pjt1.model.mapper.MainMapper">

  <select id="selectFavorite" parameterType="String" resultType="SubscriptionDto">
    select * from subscription where favorite_flag = 1 and user_id = #{user_id} and is_used=0;
  </select>
</mapper>